# Telegram Bot Token (получить через @BotFather)
BOT_TOKEN=your_bot_token_here

# Admin Telegram ID (для получения заявок и ответа клиентам)
ADMIN_ID=your_telegram_id

# Order Channel ID (для отправки заявок в канал)
# Создайте канал, добавьте туда бота как администратора и укажите ID
ORDER_CHANNEL_ID=-1001234567890

# Webhook URL (Railway установит автоматически, можно оставить пустым)
WEBHOOK_URL=

# Port (Railway установит автоматически)
PORT=3000
